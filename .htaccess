#php_flag register_globals off
#php_value upload_max_filesize 30M
#php_value post_max_size 30M

DirectoryIndex index.php index.html index.htm

Options -MultiViews
Options -Indexes

RewriteEngine on

### Routes For the API End Points

RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f

RewriteCond %{QUERY_STRING} (^|&)chat=[new,to_user,from_user](&|$)
RewriteRule ^(.*)$ /$1?chat=%1 [L]


RewriteRule    ^api/v1/user/([0-9]+)$              api/v1/controller/user.php?user_id=$1      [L,QSA]    # Handle task id requests

RewriteRule    ^api/v1/im_msg/([0-9]+)$            api/v1/controller/im_msg.php?id=$1         [L,QSA]    # Handle task id requests
RewriteRule    ^api/v1/im_msg/user_id/([0-9]+)$    api/v1/controller/im_msg.php?user_id=$1    [L,QSA]    # Handle task id requests
RewriteRule    ^api/v1/im_msg/chat/([0-9])/([0-9])/([0-9])$       api/v1/controller/im_msg.php?chat=%1       [L,QSA]    # Handle task id requests
RewriteRule    ^api/v1/im_msg$                     api/v1/controller/im_msg.php               [L,QSA]    # Handle task all requests
RewriteRule    ^api/v1/im_msg/page/([0-9]+)$       api/v1/controller/im_msg.php?page=$1       [L]    # Handle task all requests


RewriteRule    ^api/v1/admin_login/([0-9]+)$       api/v1/controller/admin_login.php?id=$1    [L,QSA]    # Handle task id requests

